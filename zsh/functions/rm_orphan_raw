echo "Remove RAW without existing JPG? "
read reply
echo

if [[ $reply =~ ^[Yy]$ ]]
then
    for filename in ./*; do
        name=${filename##*/}
        base=${name%.*}
        if [[ -z $(find .. -name "$base.*" -not -path "../RAW*") ]]; then
            echo "removing $name"
            rm $name
        fi
    done

fi
